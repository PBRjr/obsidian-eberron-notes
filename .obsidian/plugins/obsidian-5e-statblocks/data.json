        "skillsaves": {
          "Athletics": 6,
          "Perception": 3,
          "Survival": 3
        },
        "damage_resistances": "cold",
        "senses": "darkvision 60 ft., passive Perception 13",
        "languages": "Common, Giant",
        "cr": "2",
        "traits": [
          {
            "desc": "The hunter's vision is not obscured by fog, snowfall, or magical wind. They can see clearly through areas that are heavily obscured by such effects.",
            "name": "Storm Sight"
          },
          {
            "desc": "When the hunter isn't [incapacitated](Misc%20Files/CLI/rules/conditions.md#Incapacitated) and they are reduced to 0 hit points but not killed outright, they can make an attack against an enemy (no action required) before the hit point reduction is resolved. If the attack hits and its damage reduces the target to 0 hit points, the hunter drops to 1 hit point instead of 0 hit points.",
            "name": "Relentless (1/Turn)"
          }
        ],
        "actions": [
          {
            "desc": "The hunter makes two Ice-Spear attacks. If the hunter targets the same creature with both attacks, the target has disadvantage on attack rolls against creatures other than the hunter until the start of the hunter's next turn.",
            "name": "Multiattack"
          },
          {
            "desc": "Melee or Ranged Weapon Attack: +6 to hit, reach 5 ft. or range 20/60 ft., one target. Hit: 7 (1d6 + 4) piercing damage plus 3 (1d6) cold damage, and the target's speed is reduced by 10 feet until the start of the hunter's next turn.",
            "name": "Ice-Spear"
          }
        ],
        "bonus_actions": [
          {
            "desc": "The hunter magically dissolves into a swirl of snow and teleports up to 30 feet to an unoccupied space they can see.",
            "name": "Snow-Step (Recharge 5-6)"
          }
        ],
        "reactions": [
          {
            "desc": "When an enemy within 5 feet of the hunter targets another creature with an attack, the attacker must target the hunter instead.",
            "name": "No!"
          }
        ],
        "source": [
          "FleeMortals"
        ],
